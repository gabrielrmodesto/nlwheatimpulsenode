datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User{
	id 				 String @id @default(uuid())
	name 			 String
	github_id  Int
	avatar_url String
	login 		 String

	//quando criar a tabela passara esse nome
	@@map("users")
}
